@using Service.Dtos.Setting
@model SettingEditVM
<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <p class="card-description">

            </p>
            <form asp-action="Edit" method="post" enctype="multipart/form-data" class="forms-sample">

                <div class="form-group">
                    <label for="exampleInputName1">@Model.Key</label>
                    <input asp-for="Value" type="text" class="form-control" id="exampleInputName1" placeholder="Value">
                    <span asp-validation-for="Value" class="text-danger"></span>

                </div>


                <div class="input-group mb-3">
                    <label class="input-group-text" for="inputGroupFile01">Upload</label>
                    <input asp-for="UploadedImage" type="file" class="form-control" id="inputGroupFile01" />
                </div>
                <span asp-validation-for="UploadedImage" class="text-danger"></span>

                <div class="mt-3">
                    <button type="submit" class="btn btn-primary mr-2">Submit</button>
                    <button class="btn btn-light">Cancel</button>
                </div>

            </form>
        </div>
    </div>
</div>